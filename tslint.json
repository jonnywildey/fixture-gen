{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "jsx-no-lambda": false,
    "indent": [
      true,
      "spaces",
      2
    ],
    "max-line-length": [true, 180],
    "member-ordering": [false],
    "object-literal-sort-keys": false
  }
}
